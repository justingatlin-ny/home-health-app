{
  "name": "home-health-app",
  "version": "1.0.0",
  "description": "Manage documents securely",
  "main": "index.js",
  "repository": "git@github.com:justingatlin-ny/home-health-app.git",
  "author": "Justin Gatlin <vikingstamp@live.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "NODE_ENV=development concurrently \"webpack --config webpack.dom.js\" \"webpack --config webpack.server.js\"",
    "dev": "pm2 start --only documents-dev --watch --no-daemon",
    "server": "pm2 start --only documents-prod",
    "webpack:dom": "NODE_ENV=development webpack --config webpack.dom.js",
    "webpack:server": "NODE_ENV=development webpack --config webpack.server.js --watch",
    "prod:dom": "NODE_ENV=production webpack --config webpack.dom.js",
    "prod:server": "NODE_ENV=production webpack --config webpack.server.js",
    "prod:code": "NODE_ENV=production concurrently \"webpack --config webpack.dom.js\" \"webpack --config webpack.server.js\"",
    "delete": "pm2 delete all",
    "stop": "pm2 stop all --watch",
    "clean": "rm -rf htdocs",
    "clean:uploads": "rm -r uploads/*",
    "flush": "pm2 flush",
    "logs": "pm2 logs all"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "aws-amplify": "^1.1.40",
    "aws-amplify-react": "^2.4.4",
    "aws-sdk": "^2.521.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^5.0.4",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "ejs": "^2.7.1",
    "express": "^4.17.3",
    "express-subdomain": "^1.0.5",
    "file-loader": "^4.2.0",
    "helmet": "^3.20.1",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "multer": "^1.4.2",
    "react": "^16.11.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "styled-components": "^4.3.2"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.0"
  }
}
